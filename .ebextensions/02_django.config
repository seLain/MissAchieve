container_commands:
  01_makemigrates:
    command: "python manage.py makemigrates --noinput"
    leader_only: true
  02_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
  03_chown_sqlitedb:
    command: "sudo chown wsgi db.sqlite3"
    leader_only: true
  04_superuser:
    command: "python manage.py createsu"
    leader_only: true
  05_collectstatic:
    command: "python manage.py collectstatic --noinput"