container_commands:
  01_migrate:
    command: "python manage.py migrate --noinput"
    leader_only: true
  02_chown_sqlitedb:
    command: "sudo chown wsgi db.sqlite3"
    leader_only: true
  03_superuser:
    command: "python manage.py createsu"
    leader_only: true
  04_collectstatic:
    command: "python manage.py collectstatic --noinput"